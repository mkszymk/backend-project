<div class="homeTableContainer">
  <table class="homeTable">
    <thead>
      <tr>
        <th>
          ID
        </th>
        <th>
          Producto
        </th>
        <th>
          Código
        </th>
        <th>
          Categoría
        </th>
        <th>
          Stock
        </th>
        <th>
          Precio
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
        <tr>
          <td>{{this.id}}</td>
          <td>{{this.title}}</td>
          <td>{{this.code}}</td>
          <td>{{this.category}}</td>
          <td>{{this.stock}}</td>
          <td>{{this.price}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>